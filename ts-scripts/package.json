{
  "name": "injective-lists",
  "version": "1.0.0",
  "description": "A way to store different lists in JSON format.",
  "main": "index.js",
  "scripts": {
    "tokens:images": "ts-node ./uploadImages",
    "slugs:generate": "ts-node ./generateMarketSlugs",
    "tokens:static": "ts-node ./generateStaticTokens",
    "tokens:supply": "ts-node ./generateSupplyTokens",
    "tokens:external": "ts-node ./generateExternalTokens",
    "fetch:chain:metadata": "ts-node ./fetchChainTokenMetadata",
    "tokens:supply:clean": "ts-node ./generateSupplyTokens --clean",
    "tokens:external:clean": "ts-node ./generateExternalTokens --clean",
    "tokens:generate": "yarn fetch:chain:metadata && yarn tokens:static && yarn tokens:external && yarn tokens:supply && yarn tokens:images && ts-node ./generateTokens",
    "tokens:generate:flush": "yarn fetch:chain:metadata && yarn tokens:static && yarn tokens:external:clean && yarn tokens:supply:clean && yarn tokens:images && ts-node ./generateTokens"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.11.30",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "alchemy-sdk": "^2.6.3"
  },
  "devDependencies": {
    "@injectivelabs/prettier-config": "1.0.3",
    "@injectivelabs/utils": "1.14.11-beta.3",
    "@injectivelabs/networks": "1.14.11-beta.5",
    "@injectivelabs/sdk-ts": "1.14.11-beta.47",
    "@injectivelabs/token-metadata": "1.14.11-beta.36"
  }
}
