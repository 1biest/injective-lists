{
  "name": "injective-lists",
  "version": "1.0.0",
  "description": "A way to store different lists in JSON format.",
  "main": "index.js",
  "scripts": {
    "tokens:images": "ts-node ./uploadImages",
    "validators:images": "ts-node ./uploadValidatorImages",
    "check:duplicates": "ts-node ./duplicateCheck",
    "slugs:generate": "ts-node ./generateMarketSlugs",
    "marketMap:generate": "ts-node ./generateMarketMap",
    "tokens:static": "ts-node ./generateStaticTokens",
    "tokens:supply": "ts-node ./generateSupplyTokens",
    "tokens:external": "ts-node ./generateExternalTokens",
    "fetch:mito:vaults": "ts-node ./fetchMitoVaults",
    "fetch:chain:metadata": "ts-node ./fetchChainTokenMetadata",
    "tokens:factory:generate": "ts-node ./generateFactoryTokens",
    "tokens:mito:generate": "ts-node ./generateMitoTokens",
    "wasm:query:generate": "ts-node ./generateWasmQueryMessages",
    "wasm:execute:generate": "ts-node ./generateWasmExecuteMessages",
    "wasm:fetch:codeIds": "ts-node ./fetchCodeIds",
    "wasm:generate": "yarn wasm:fetch:codeIds && yarn wasm:query:generate && yarn wasm:execute:generate",
    "wallets:ofac": "ts-node ./generateOfacList",
    "verifiedDenoms:generate": "ts-node ./generateVerifiedDenoms",
    "validators:generate": "yarn validators:images && ts-node ./generateValidatorsData",
    "restriction:generate": "ts-node ./generateRestrictionList",
    "tokens:generate": "yarn fetch:chain:metadata && yarn marketMap:generate && yarn fetch:mito:vaults && yarn tokens:static && yarn tokens:factory:generate && yarn tokens:external && yarn tokens:mito:generate && yarn tokens:supply && yarn tokens:images && ts-node ./generateTokens",
    "tokens:generate:flush": "ts-node ./flushCw20Cache && yarn tokens:generate",
    "wasm:generate:force": "yarn wasm:fetch:codeIds && yarn wasm:query:generate -f && yarn wasm:execute:generate -f"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.11.30",
    "alchemy-sdk": "^2.6.3",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@injectivelabs/utils": "1.14.14-beta.3",
    "@injectivelabs/networks": "1.14.14-beta.3",
    "@injectivelabs/sdk-ts": "1.14.14-beta.8",
    "@injectivelabs/prettier-config": "1.0.3"
  }
}
